local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/NICKISBAD/Nick-s-Modded-KAVO-Lib/main/Nick'sModdedKavoLib.lua"))()
local Window = Library.CreateLib("Un Hub: Fe | Make by LE_GO89", "DarkTheme")
local player = game.Players.LocalPlayer

local Tab = Window:NewTab("Fe Animation")
local Section = Tab:NewSection("Fe Animation")

local function createAnimationToggle(name, description, animationUrl)
    Section:NewToggle(name, description, function(state)
        local url = state and animationUrl or "https://pastebin.com/raw/4KTkXYkZ"
        loadstring(game:HttpGet(url))()
    end)
end

createAnimationToggle("[Fe] Ninja", "Make your animation to Ninja animation.", "https://pastebin.com/raw/dUbsqd0z")
